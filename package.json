{
  "name": "breakpoint-exporter",
  "displayName": "Breakpoint Exporter",
  "description": "Export and import breakpoints",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished:breakpoint-exporter.export",
    "onStartupFinished:breakpoint-exporter.import"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "breakpoint-exporter.export",
        "title": "Export Breakpoints"
      },
      {
        "command": "breakpoint-exporter.import",
        "title": "Import Breakpoints"
      }
    ]
    
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.3",
    "@vscode/test-electron": "^2.2.0"
  }
}
